    "maintainer": {
        "role": "Maintainer",
        "id": "maintainer",
        "desc": "项目的规划和设计者，拥有合并主干分支的权限，从 Committer 中产生。他们必须对子项目的健康表现出良好的判断力和责任感。维护者必须直接或通过委派这些职责来设置技术方向并为子项目做出或批准设计决策。",
        "detail": [
            {
                "title": "如何成为 TiDB Maintainer：",
                "lists": [
                    "对 TiDB 的技术目标和方向有非常深入的了解",
                    "积极组织技术研讨会，并提出有建设性的专案",
                    "能处理代码实现过程中的复杂问题",
                    "PMC 内的成员投票一致通过"
                ]
            },
            {
                "title": "Maintainer 义务及权利：",
                "lists": [
                    "制定子项目的技术设计和决策。",
                    "设置子项目的技术方向和优先级。",
                    "参与版本发布，规划项目发展路线",
                    "指导 Contributor，Reviewer 和 Committer 对项目持续贡献",
                    "保证子项目的整体质量"
                ]
            }
        ]
    },

    "committer": {
        "role": "Committer",
        "id": "committer",
        "desc": "由 Maintainer 或 PMC 推荐，是对 TiDB 有突出贡献的 Contributor。需要独立完成至少一个 feature 或修复重大 bug。",
        "detail": [
            {
                "title": "如何成为 Committer：",
                "lists": [
                    "表现出良好的技术判断力",
                    "至少是两个以上子项目的 Reviewer",
                    "审阅或合并了一定数量的 TiDB 代码",
                    "对 TiDB 性能提升做出突出贡献；修复了重大 bug",
                    "由两名或以上在任 Maintainer 提名通过"
                ]
            },
            {
                "title": "Committer 义务及权利：",
                "lists": [
                    "整体把控项目的代码质量 ",
                    "指导 Contributor 和 Reviewer 对 TiDB 持续贡献",
                    "定期参与 Committer 会议，对项目发展有优先知情权"
                ]
            }
        ]
    },

    "reviewer": {
        "role": "Reviewer",
        "id": "reviewer",
        "desc": "负责审查社区提交的代码，保证代码的质量和正确性，从 Contributor 中产生。",
        "detail": [
            {
                "title": "如何成为 Reviewer：",
                "lists": [
                    "熟悉代码库",
                    "为 TiDB 子模块贡献一定数量的代码",
                    "在专项兴趣小组（SIG）中表现优秀",
                    "获得至少两位在任 Committer 的提名通过"
                ]
            },
            {
                "title": "Reviewer 义务及权利：",
                "lists": [
                    "保证子项目代码的正确性",
                    "及时响应社区提交的 PR",
                    "定期参加专项兴趣小组（SIG）的研讨会",
                    "对代码拥有 LGTM 的权限"
                ]
            }
        ]
    },
    
